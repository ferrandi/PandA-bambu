	  mingw*) \
	    compile_options='$(CLANG10_LIBS) -lversion `$(I386_LLVM_CONFIG10_EXE) --ldflags --libs --system-libs` -lbdd';\
	    compile_options='';; \
	  darwin*) \
	    compile_options='-fPIC -shared -undefined dynamic_lookup -fno-rtti -lbdd';; \
